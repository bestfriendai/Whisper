# WhisperDate Environment Configuration
# Copy this file to .env and fill in your actual values

# Firebase Configuration
FIREBASE_API_KEY_WEB=your_web_api_key_here
FIREBASE_API_KEY_ANDROID=your_android_api_key_here
FIREBASE_API_KEY_IOS=your_ios_api_key_here
FIREBASE_APP_ID_WEB=your_web_app_id_here
FIREBASE_APP_ID_ANDROID=your_android_app_id_here
FIREBASE_APP_ID_IOS=your_ios_app_id_here
FIREBASE_PROJECT_ID=your_project_id_here
FIREBASE_AUTH_DOMAIN=your_auth_domain_here
FIREBASE_STORAGE_BUCKET=your_storage_bucket_here
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
FIREBASE_MEASUREMENT_ID=your_measurement_id_here

# Google Sign-In
GOOGLE_CLIENT_ID_IOS=your_ios_client_id_here
GOOGLE_CLIENT_ID_ANDROID=your_android_client_id_here
GOOGLE_CLIENT_ID_WEB=your_web_client_id_here

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
MAPS_API_KEY=your_google_maps_api_key_here

# App Configuration
APP_NAME=WhisperDate
APP_VERSION=1.0.0
APP_BUILD_NUMBER=1
APP_BUNDLE_ID_IOS=com.yourcompany.whisperdate
APP_PACKAGE_NAME_ANDROID=com.yourcompany.whisperdate

# Environment
ENVIRONMENT=development # development, staging, production
DEBUG_MODE=true
ENABLE_CRASHLYTICS=false
ENABLE_ANALYTICS=false

# Security
JWT_SECRET_KEY=your_jwt_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here

# Rate Limiting
MAX_REVIEWS_PER_DAY=10
MAX_MESSAGES_PER_HOUR=100
MAX_LOGIN_ATTEMPTS=5

# Content Moderation
ENABLE_CONTENT_MODERATION=true
PROFANITY_FILTER=true
IMAGE_MODERATION=true

# Feature Flags
ENABLE_CHAT=true
ENABLE_VIDEO_CALLS=false
ENABLE_PREMIUM_FEATURES=true
ENABLE_AI_MATCHING=false

# Monitoring
SENTRY_DSN=your_sentry_dsn_here
LOG_LEVEL=debug # debug, info, warning, error